{
  "name": "@enso/contracts",
  "version": "0.1.3",
  "description": "Enso protocol smart contracts",
  "main": "./dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "start": "yarn hardhat node --fork https://eth-mainnet.alchemyapi.io/v2/5IIoZbDFo-8QzhVpR2JDHma79j9_HbIX --hostname 0.0.0.0",
    "build": "yarn hardhat compile && tsc --build",
    "coverage": "rm -r artifacts && rm -r typechain && TS_NODE_TRANSPILE_ONLY=true yarn hardhat coverage",
    "deploy": "yarn hardhat run scripts/full-deploy-script.js --network",
    "register-tokens": "yarn hardhat run scripts/register-token.ts --network",
    "register-dictionary": "yarn hardhat run scripts/register-dictionary.js --network",
    "fill-ui": "yarn hardhat run scripts/fill-ui-script.js --network",
    "lint": "yarn eslint",
    "prettier:solidity": "yarn prettier --write 'contracts/**/*.sol'",
    "solhint": "solhint -f table --fix 'contracts/**/*.sol'",
    "test": "yarn hardhat test",
    "docs": "./scripts/build_docs.sh"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnsoFinance/v1-core"
  },
  "files": [
    "contracts",
    "artifacts"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EnsoFinance/v1-core/issues"
  },
  "homepage": "https://github.com/EnsoFinance/v1-core#readme",
  "dependencies": {
    "@0xsequence/token-directory": "1.1.13",
    "@balancer-labs/core": "https://github.com/balancer-labs/balancer-core",
    "@chainlink/contracts": "~0.1.6",
    "@ethersproject/abstract-signer": "^5.0.6",
    "@ethersproject/bignumber": "^5.0.8",
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "~2.0.1",
    "@openzeppelin/contracts": "~3.3.0",
    "@typechain/ethers-v5": "6.0.5",
    "@typechain/hardhat": "^1.0.1",
    "@types/chai": "~4.2.16",
    "@types/fs-extra": "~9.0.10",
    "@types/mocha": "~8.2.2",
    "@types/node": "~14.14.37",
    "@uniswap/lib": "~4.0.1-alpha",
    "@uniswap/v2-core": "~1.0.1",
    "@uniswap/v2-periphery": "~1.1.0-beta.0",
    "@uniswap/v3-core": "~1.0.0",
    "@uniswap/v3-periphery": "~1.1.1",
    "chai": "~4.3.4",
    "dotenv": "~8.2.0",
    "eslint": "~7.14.0",
    "ethereum-waffle": "^3.2.0",
    "ethers": "^5.0.24",
    "hardhat": "^2.0.10",
    "prettier": "~2.2.1",
    "prettier-plugin-solidity": "~1.0.0-beta.5",
    "sol2uml": "~1.1.25",
    "solhint": "~3.3.2",
    "solidity-coverage": "~0.7.17",
    "solidity-docgen": "~0.5.11",
    "ts-node": "~9.1.1",
    "typechain": "~3.0.0",
    "typescript": "~4.2.3"
  }
}
